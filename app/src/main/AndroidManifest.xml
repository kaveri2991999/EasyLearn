        <activity android:name=".PDFCE3Opener" />
        <activity android:name=".PDFCE3Activity" />
        <activity android:name=".PDFCE2Opener" />
        <activity android:name=".PdfCE2Activity" />
        <activity android:name=".PDFOpener" />
        <activity android:name=".PdfActivity" />
        <activity android:name=".YearActivity" />
        <activity android:name=".BranchActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>